<h1> Edit User </h1>
<% if logged_in? %>
<form action="/signup" method="POST">
<table> 
    <tr>
        <td> First Name: </td>
        <td><input type="text" name="user[first_name]"></td>
    </tr>
    <tr>    
        <td>Last Name: </td>
        <td><input type="text" name="user[last_name]"></td>
    </tr>
    <tr>
        <td>Email: </td>
        <td><input type="email" name="user[email]"></td>
    </tr>
    <tr>
        <td>Password: </td>
        <td><input type="password" name="user[password]"></td>
    </tr>
    <tr>
        <td></td>
        <td align="right"><input type="submit" value="Sign Up"></td>
    </tr>
</table>
</form>
<% else %>
Please Sign In: <a href="/"> Here </a>
<% end %>